{
  "name": "utils-is",
  "version": "1.0.0",
  "description": "A Set of `is` functions in JavaScript for checking on objects and types",
  "main": "index.js",
  "repository": "git@github.com:AlaeddineMessadi/utils-is.git",
  "author": "Alaeddine Messadi <alaeddine.messadi@gmail.com> (https://almessadi.com)",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "commit": "cz",
    "eslint": "eslint '**/*.js' --ignore-pattern node_modules/",
    "eslint:fix": "eslint '**/*.js' --ignore-pattern node_modules/ --fix"
  },
  "bugs": {
    "url": "https://github.com/AlaeddineMessadi/utils-is/issues"
  },
  "devDependencies": {
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
